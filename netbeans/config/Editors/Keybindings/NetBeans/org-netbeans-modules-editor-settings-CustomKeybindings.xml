<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE bindings PUBLIC "-//NetBeans//DTD Editor KeyBindings settings 1.1//EN" "http://www.netbeans.org/dtds/EditorKeyBindings-1_1.dtd">
<bindings>
    <bind actionName="move-selection-else-line-down" key="SD-DOWN"/>
    <bind actionName="move-selection-else-line-up" key="SD-UP"/>
    <bind actionName="toggle-comment" key="SD-COLON"/>
    <bind actionName="zoom-text-in" key="D-MOUSE_WHEEL_UP"/>
    <bind actionName="zoom-text-out" key="D-MOUSE_WHEEL_DOWN"/>
    <bind key="SD-C" remove="true"/>
    <bind key="INSERT$SEMICOLON" remove="true"/>
    <bind key="O-BACK_SPACE" remove="true"/>
    <bind key="D-J" remove="true"/>
    <bind key="S-SPACE" remove="true"/>
    <bind key="O-MOUSE_WHEEL_DOWN" remove="true"/>
    <bind key="D-P" remove="true"/>
    <bind key="SO-KP_DOWN" remove="true"/>
    <bind key="SD-KP_UP" remove="true"/>
    <bind key="S-F10" remove="true"/>
    <bind key="INSERT$P" remove="true"/>
    <bind key="SO-UP" remove="true"/>
    <bind key="SD-KP_DOWN" remove="true"/>
    <bind key="O-MOUSE_WHEEL_UP" remove="true"/>
    <bind key="D-B" remove="true"/>
    <bind key="SO-KP_UP" remove="true"/>
    <bind key="SO-DOWN" remove="true"/>
    <bind key="INSERT$V" remove="true"/>
    <bind key="D-DOWN" remove="true"/>
</bindings>
